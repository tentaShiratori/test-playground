    "jotai": "^2.6.5",
    "lodash-es": "^4.17.21",
    "msw": "^2.2.1",
    "next": "14.2.15",
    "node-fetch-commonjs": "^3.3.2",
    "nyc": "^15.1.0",
    "open-cli": "^8.0.0",
    "pathpida": "^0.22.0",
    "pixi.js": "^7.3.0",
    "playwright-test-coverage": "^1.2.12",
    "react": "canary",
