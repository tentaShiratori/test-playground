{
  "name": "test-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fmt": "biome check --apply .",
    "pathpida": "pathpida --enableStatic",
    "depcheck": "depcheck",
    "sb:build": "storybook build",
    "sb:start": "storybook dev -p 6006",
    "test": "run-p test:*",
    "test:jest": "jest",
    "test:sb": "concurrently -k -P -s first -n \"SB,TEST\" \"pnpm:sb:start\" \"pnpm wait-on tcp:127.0.0.1:6006 && pnpm test-storybook -- {@}\" -- ",
    "test:e2e": "playwright test",
    "precov": "rimraf coverage",
    "cov": "run-p cov:*",
    "cov:jest": "nyc jest --coverage",
    "cov:e2e": "cross-env COVERAGE=1 ISTANBUL_TEMP_DIR=coverage/results playwright test",
    "cov:sb": "npm run test:sb -- --coverage --coverageDirectory=coverage/results",
    "cov-report": "nyc report --report-dir coverage & open-cli ./coverage/lcov-report/index.html",
    "cov-check": "nyc check-coverage",
    "db:generage": "drizzle-kit generate",
    "db:migrate": "drizzle-kit push"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@biomejs/biome": "^1.5.3",
    "@conform-to/react": "^1.2.2",
